#my name 
from itertools import combinations

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, n): 
        if n % i == 0:
            return False
    return True

prime_numbers = [num for num in range(1, 1001) if is_prime(num)]

prime_numbers_sequence = []
n = len(prime_numbers)

for start in range(n):
    for end in range(start + 2, n + 1):
        sequence = prime_numbers[start:end]
        total = sum(sequence)
        if is_prime(total):
            prime_numbers_sequence.append(sequence)

print (prime_numbers_sequence)
